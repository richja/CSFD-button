{
	"name": "Titulky plus",
	"author": "Jan Richter aka richja",
	"version": "0.0.1",
	"description": "Vylepšení webu titulky.com pro pohodlnější práci",
	"manifest_version": 2,
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	},
	"permissions": ["http://csfdapi.cz/"],
	"web_accessible_resources": [
		"*.png"
	],
	"content_scripts": [
	{
		"matches": ["http://www.titulky.com/*"],
		"js": ["jquery.min.js","content.js"],
		"css": ["cstyle.css"]
	}
	]
}
